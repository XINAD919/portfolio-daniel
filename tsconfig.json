{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "components/icons/D.jsx", "components/icons/GitHub.jsx", "components/icons/Hamburger.jsx", "components/icons/Lang.jsx", "components/icons/Linkedin.jsx", "components/icons/Outlook.jsx", "components/icons/WhatsApp.jsx"],
  "exclude": ["node_modules"]
}
